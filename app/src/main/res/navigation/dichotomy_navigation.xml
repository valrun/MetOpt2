<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dichotomy_navigation"
    app:startDestination="@id/dichotomyFragment"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/dichotomyFragment"
        android:name="com.example.metopt.fragments.dichotomyFragment"
        android:label="dichotomyFragment"
        tools:layout="@layout/fragment_dichotomy">
        <action
            android:id="@+id/action_dichotomy_to_new"
            app:destination="@id/dichotomyFragment" />
        <argument
            android:name="count"
            android:defaultValue="0"
            app:argType="string" />
    </fragment>
</navigation>
